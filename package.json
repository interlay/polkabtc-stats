{
    "name": "@interlay/polkabtc-stats",
    "version": "1.0.0",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "file": ["dist/"],
    "scripts": {
        "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
        "build": "tsoa spec-and-routes && tsc",
        "client": "openapi-generator-cli generate -i build/swagger.json -g typescript-axios -o client && tsc -d client/*.ts --outdir dist",
        "start": "node build/src/server.js"
    },
    "dependencies": {
        "axios": "^0.21.1"
    },
    "devDependencies": {
        "tsoa": "^3.4.0",
        "body-parser": "^1.19.0",
        "express": "^4.17.1",
        "pg": "^8.5.1",
        "swagger-ui-express": "^4.1.6",
        "@openapitools/openapi-generator-cli": "^2.1.17",
        "@types/body-parser": "^1.19.0",
        "@types/express": "^4.17.11",
        "@types/node": "^14.14.20",
        "@types/pg": "^7.14.8",
        "@types/swagger-ui-express": "^4.1.2",
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    }
}
